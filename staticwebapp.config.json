{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["*.{css,scss,js,png,gif,ico,jpg,svg,woff,woff2,ttf,eot}"]
  },
  "routes": [
    {
      "route": "/",
      "redirect": "/index.html",
      "statusCode": 302
    },
    {
      "route": "/*.html",
      "headers": {
        "cache-control": "public, max-age=3600, must-revalidate"
      }
    },
    {
      "route": "/api/*",
      "allowedRoles": ["authenticated", "anonymous"]
    }
  ],
  "responseOverrides": {
    "400": {
      "rewrite": "/error.html",
      "statusCode": 400
    },
    "401": {
      "redirect": "/index.html",
      "statusCode": 302
    },
    "403": {
      "rewrite": "/error.html",
      "statusCode": 403
    },
    "404": {
      "rewrite": "/404.html",
      "statusCode": 404
    }
  },
  "globalHeaders": {
    "content-security-policy": "default-src 'self' https://cdn.plot.ly https://*.plotly.com 'unsafe-inline' 'unsafe-eval'; img-src 'self' data: https:; font-src 'self' data: https:;",
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "SAMEORIGIN",
    "Referrer-Policy": "strict-origin-when-cross-origin"
  },
  "mimeTypes": {
    ".html": "text/html",
    ".css": "text/css",
    ".js": "application/javascript",
    ".json": "application/json"
  },
  "platform": {
    "apiRuntime": "python:3.11"
  }
}
